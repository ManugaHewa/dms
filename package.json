{
  "name": "dms",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/node": "^20.19.14",
    "npm-run-all": "4.1.5",
    "tsx": "4.19.1",
    "turbo": "2.0.6",
    "typescript": "5.6.2"
  },
  "scripts": {
    "dev:infra": "docker compose -f dms/infra/compose/docker-compose.dev.yml up -d",
    "dev:db:migrate": "pnpm --filter @dms/db migrate",
    "dev:db:seed": "pnpm --filter @dms/db seed",
    "dev:api": "pnpm --filter @dms/api dev",
    "dev:worker": "pnpm --filter @dms/worker dev",
    "dev": "run-p dev:infra dev:db:migrate dev:api dev:worker",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test"
  }
}
